      "cell_type": "code",
      "source": [
        "import tensorflow as tf\n",
        "from tensorflow.keras.preprocessing.text import Tokenizer\n",
        "from tensorflow.keras.preprocessing.sequence import pad_sequences\n",
        "\n",
        "# Load IMDb dataset\n",
        "imdb = tf.keras.datasets.imdb\n",
        "(train_data, train_labels), (test_data, test_labels) = imdb.load_data(num_words=30000)\n",
        "\n",
        "print(f\"Length of training Data is {train_data}\")\n",
        "\n",
        "# Split the training data into training and validation sets\n",
        "validation_split = 0.3\n",
        "num_validation_samples = int(validation_split * len(train_data))\n",
        "\n",
        "x_train = train_data[:-num_validation_samples]\n",
        "y_train = train_labels[:-num_validation_samples]\n",
        "x_val = train_data[-num_validation_samples:]\n",
        "y_val = train_labels[-num_validation_samples:]\n",
        "\n",
        "# Convert integer sequences back to text\n",
        "word_index = imdb.get_word_index()\n",
        "word_index_reverse = dict([(value, key) for (key, value) in word_index.items()])\n",
        "train_texts = [' '.join([word_index_reverse.get(i - 3, '') for i in sequence]) for sequence in x_train]\n",
        "val_texts = [' '.join([word_index_reverse.get(i - 3, '') for i in sequence]) for sequence in x_val]\n",
        "test_texts = [' '.join([word_index_reverse.get(i - 3, '') for i in sequence]) for sequence in test_data]\n",
        "\n",
        "# Preprocess the data\n",
        "max_len = 256  # Maximum sequence length\n",
        "trunc_type = 'post'\n",
        "padding_type = 'post'\n",
        "oov_tok = \"<OOV>\"\n",
        "\n",
        "tokenizer = Tokenizer(num_words=30000, oov_token=oov_tok)\n",
        "tokenizer.fit_on_texts(train_texts)\n",
        "\n",
        "word_index = tokenizer.word_index\n",
        "\n",
        "train_sequences = tokenizer.texts_to_sequences(train_texts)\n",
        "train_padded = pad_sequences(train_sequences, maxlen=max_len, padding=padding_type, truncating=trunc_type)\n",
        "\n",
        "val_sequences = tokenizer.texts_to_sequences(val_texts)\n",
        "val_padded = pad_sequences(val_sequences, maxlen=max_len, padding=padding_type, truncating=trunc_type)\n",
        "\n",
        "test_sequences = tokenizer.texts_to_sequences(test_texts)\n",
        "test_padded = pad_sequences(test_sequences, maxlen=max_len, padding=padding_type, truncating=trunc_type)\n",
        "\n",
        "# Build a more complex RNN model with added dropout layers, early stopping, and gradient clipping\n",
        "model = tf.keras.Sequential([\n",
        "    tf.keras.layers.Embedding(30000, 32, input_length=max_len),  # Increased embedding dimension\n",
        "    tf.keras.layers.Dropout(0.3),  # Increased dropout rate\n",
        "    tf.keras.layers.Bidirectional(tf.keras.layers.LSTM(128, return_sequences=True)),  # Increased LSTM units\n",
        "    tf.keras.layers.Dropout(0.3),  # Increased dropout rate\n",
        "    tf.keras.layers.Bidirectional(tf.keras.layers.LSTM(64)),\n",
        "    tf.keras.layers.Dropout(0.2),  # Increased dropout rate\n",
        "    tf.keras.layers.Dense(64, activation='relu'),  # Increased dense layer size\n",
        "    tf.keras.layers.Dropout(0.1),  # Increased dropout rate\n",
        "    tf.keras.layers.Dense(1, activation='sigmoid')\n",
        "])\n",
        "\n",
        "# Add gradient clipping\n",
        "optimizer = tf.keras.optimizers.Adam(clipvalue=0.5)\n",
        "model.compile(loss='binary_crossentropy', optimizer=optimizer, metrics=['accuracy'])\n",
        "\n",
        "# Implement early stopping\n",
        "early_stopping = tf.keras.callbacks.EarlyStopping(monitor='val_loss', patience=3, restore_best_weights=True)\n",
        "\n",
        "# Train the model\n",
        "num_epochs = 30  # Increased number of epochs\n",
        "history = model.fit(train_padded, y_train, epochs=num_epochs, validation_data=(val_padded, y_val), callbacks=[early_stopping])\n",
        "\n",
        "# Evaluate the model\n",
        "test_loss, test_acc = model.evaluate(test_padded, test_labels)\n",
        "print(\"Test Accuracy:\", test_acc)\n"
      ],
